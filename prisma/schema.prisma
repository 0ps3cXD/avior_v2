// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  username String @unique
  displayname String
  id String @unique
  avatarurl String
  messagecount Int @default(0)
}

model Team {
  username String @unique
  id String @unique
  permissionlevel Int
}

model Locked {
  id String @unique
  locked Boolean
}

model TemporaryVoice {
  id String @unique
  name String
  owner String @unique
  private Boolean @default(false)
}

model Ticket {
  id String @unique
  name String
  interactor String 
}

model Guild {
  key String @unique
  value String
}